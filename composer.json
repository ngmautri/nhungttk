{
	"name" : "zendframework/skeleton-application",
	"description" : "Skeleton Application for ZF2",
	"license" : "BSD-3-Clause",
	"keywords" : [
		"framework",
		"zf2"
	],
	"homepage" : "http://framework.zend.com/",
	"require" : {
		"php" : ">=5.6",
		"phpoffice/phpspreadsheet" : "1.3.*",
		"zendframework/zendframework" : "~2.5.3",
		"doctrine/doctrine-orm-module" : "0.8.*",
		"tecnickcom/tcpdf" : "~6.3",
		"symfony/workflow" : "~3.3",
		"symfony/event-dispatcher" : "~3.4.32",
		"ramsey/uuid" : "~3.8",
		"moneyphp/money" : "~3.2",
		"endroid/qr-code" : "~2.5.1"
	},
	"minimum-stability" : "beta",
	"autoload" : {
		"psr-0" : {
			"Inventory" : "module/Inventory/src",
			"Application" : "module/Application/src",
			"ApplicationTest" : "module/Application/test",
			"InventoryTest" : "module/Inventory/test",
			"Procure" : "module/Procure/src",
			"ProcureTest" : "module/Procure/test"
		}
	}
}